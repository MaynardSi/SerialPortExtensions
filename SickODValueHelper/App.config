<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="DeviceGroup">
      <sectionGroup name="SerialPort">
        <section name="SickODValue" type="System.Configuration.NameValueSectionHandler" />
      </sectionGroup>
    </sectionGroup>
  </configSections>

  <DeviceGroup>
    <SerialPort>
      <SickODValue>
        <add key="PortName" value="COM1"></add>
        <add key="BaudRate" value="9600"></add>
        <add key="Parity" value="None"></add>
        <add key="DataBits" value="8"></add>
        <add key="StopBits" value="One"></add>
        <add key="Handshake" value="None"></add>
        <add key="ReadTimeout" value="-1"></add>
        <add key="WriteTimeout" value="-1"></add>
      </SickODValue>
    </SerialPort>
  </DeviceGroup>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
</configuration>